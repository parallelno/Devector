.SH NAME
SDL_TransferCharacteristics \- The color transfer characteristics\[char46]
.SH HEADER FILE
Defined in SDL3/SDL_pixels\[char46]h

.SH SYNOPSIS
.nf
.B #include \(dqSDL3/SDL.h\(dq
.PP
.BI "typedef enum SDL_TransferCharacteristics
.BI "{
.BI "    SDL_TRANSFER_CHARACTERISTICS_UNKNOWN = 0,
.BI "    SDL_TRANSFER_CHARACTERISTICS_BT709 = 1,         /**< Rec. ITU-R BT.709-6 / ITU-R BT1361 */
.BI "    SDL_TRANSFER_CHARACTERISTICS_UNSPECIFIED = 2,
.BI "    SDL_TRANSFER_CHARACTERISTICS_GAMMA22 = 4,       /**< ITU-R BT.470-6 System M / ITU-R BT1700 625 PAL & SECAM */
.BI "    SDL_TRANSFER_CHARACTERISTICS_GAMMA28 = 5,       /**< ITU-R BT.470-6 System B, G */
.BI "    SDL_TRANSFER_CHARACTERISTICS_BT601 = 6,         /**< SMPTE ST 170M / ITU-R BT.601-7 525 or 625 */
.BI "    SDL_TRANSFER_CHARACTERISTICS_SMPTE240 = 7,      /**< SMPTE ST 240M */
.BI "    SDL_TRANSFER_CHARACTERISTICS_LINEAR = 8,
.BI "    SDL_TRANSFER_CHARACTERISTICS_LOG100 = 9,
.BI "    SDL_TRANSFER_CHARACTERISTICS_LOG100_SQRT10 = 10,
.BI "    SDL_TRANSFER_CHARACTERISTICS_IEC61966 = 11,     /**< IEC 61966-2-4 */
.BI "    SDL_TRANSFER_CHARACTERISTICS_BT1361 = 12,       /**< ITU-R BT1361 Extended Colour Gamut */
.BI "    SDL_TRANSFER_CHARACTERISTICS_SRGB = 13,         /**< IEC 61966-2-1 (sRGB or sYCC) */
.BI "    SDL_TRANSFER_CHARACTERISTICS_BT2020_10BIT = 14, /**< ITU-R BT2020 for 10-bit system */
.BI "    SDL_TRANSFER_CHARACTERISTICS_BT2020_12BIT = 15, /**< ITU-R BT2020 for 12-bit system */
.BI "    SDL_TRANSFER_CHARACTERISTICS_PQ = 16,           /**< SMPTE ST 2084 for 10-, 12-, 14- and 16-bit systems */
.BI "    SDL_TRANSFER_CHARACTERISTICS_SMPTE428 = 17,     /**< SMPTE ST 428-1 */
.BI "    SDL_TRANSFER_CHARACTERISTICS_HLG = 18,          /**< ARIB STD-B67, known as "hybrid log-gamma" (HLG) */
.BI "    SDL_TRANSFER_CHARACTERISTICS_CUSTOM = 31
.BI "} SDL_TransferCharacteristics;
