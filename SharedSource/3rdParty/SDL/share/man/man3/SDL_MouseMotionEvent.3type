.SH NAME
SDL_MouseMotionEvent \- Mouse motion event structure (event\[char46]motion\[char46]*)
.SH HEADER FILE
Defined in SDL3/SDL_events\[char46]h

.SH SYNOPSIS
.nf
.B #include \(dqSDL3/SDL.h\(dq
.PP
.BI "typedef struct SDL_MouseMotionEvent
.BI "{
.BI "    SDL_EventType type; /**< ::SDL_EVENT_MOUSE_MOTION */
.BI "    Uint32 reserved;
.BI "    Uint64 timestamp;   /**< In nanoseconds, populated using SDL_GetTicksNS() */
.BI "    SDL_WindowID windowID; /**< The window with mouse focus, if any */
.BI "    SDL_MouseID which;  /**< The mouse instance id, SDL_TOUCH_MOUSEID, or SDL_PEN_MOUSEID */
.BI "    Uint32 state;       /**< The current button state */
.BI "    float x;            /**< X coordinate, relative to window */
.BI "    float y;            /**< Y coordinate, relative to window */
.BI "    float xrel;         /**< The relative motion in the X direction */
.BI "    float yrel;         /**< The relative motion in the Y direction */
.BI "} SDL_MouseMotionEvent;
