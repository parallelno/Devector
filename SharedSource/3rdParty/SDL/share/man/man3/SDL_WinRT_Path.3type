.TH SDL_WinRT_Path 3type "SDL 3.1.2" "Simple Directmedia Layer" "SDL3 DATATYPES"
.SH NAME
SDL_WinRT_Path \- WinRT / Windows Phone path types
.SH HEADER FILE
Defined in SDL3/SDL_system\[char46]h

.SH SYNOPSIS
.nf
.B #include \(dqSDL3/SDL.h\(dq
.PP
.BI "typedef enum SDL_WinRT_Path
.BI "{
.BI "    /** The installed app's root directory.
.BI "        Files here are likely to be read-only. */
.BI "    SDL_WINRT_PATH_INSTALLED_LOCATION,
.BI "
.BI "    /** The app's local data store.  Files may be written here */
.BI "    SDL_WINRT_PATH_LOCAL_FOLDER,
.BI "
.BI "    /** The app's roaming data store.  Unsupported on Windows Phone.
.BI "        Files written here may be copied to other machines via a network
.BI "        connection.
.BI "    */
.BI "    SDL_WINRT_PATH_ROAMING_FOLDER,
.BI "
.BI "    /** The app's temporary data store.  Unsupported on Windows Phone.
.BI "        Files written here may be deleted at any time. */
.BI "    SDL_WINRT_PATH_TEMP_FOLDER
.BI "} SDL_WinRT_Path;
.fi
