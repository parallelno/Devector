.TH SDL_hid_bus_type 3type "SDL 3.1.2" "Simple Directmedia Layer" "SDL3 DATATYPES"
.SH NAME
SDL_hid_bus_type \- HID underlying bus types\[char46]
.SH HEADER FILE
Defined in SDL3/SDL_hidapi\[char46]h

.SH SYNOPSIS
.nf
.B #include \(dqSDL3/SDL.h\(dq
.PP
.BI "typedef enum SDL_hid_bus_type {
.BI "    /** Unknown bus type */
.BI "    SDL_HID_API_BUS_UNKNOWN = 0x00,
.BI "
.BI "    /** USB bus
.BI "       Specifications:
.BI "       https://usb.org/hid */
.BI "    SDL_HID_API_BUS_USB = 0x01,
.BI "
.BI "    /** Bluetooth or Bluetooth LE bus
.BI "       Specifications:
.BI "       https://www.bluetooth.com/specifications/specs/human-interface-device-profile-1-1-1/
.BI "       https://www.bluetooth.com/specifications/specs/hid-service-1-0/
.BI "       https://www.bluetooth.com/specifications/specs/hid-over-gatt-profile-1-0/ */
.BI "    SDL_HID_API_BUS_BLUETOOTH = 0x02,
.BI "
.BI "    /** I2C bus
.BI "       Specifications:
.BI "       https://docs.microsoft.com/previous-versions/windows/hardware/design/dn642101(v=vs.85) */
.BI "    SDL_HID_API_BUS_I2C = 0x03,
.BI "
.BI "    /** SPI bus
.BI "       Specifications:
.BI "       https://www.microsoft.com/download/details.aspx?id=103325 */
.BI "    SDL_HID_API_BUS_SPI = 0x04
.BI "
.BI "} SDL_hid_bus_type;
.fi
