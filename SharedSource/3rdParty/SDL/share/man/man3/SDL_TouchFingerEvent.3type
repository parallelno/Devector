.BI "    float dy;           /**< Normalized in the range -1...1 */
.BI "    float pressure;     /**< Normalized in the range 0...1 */
.BI "    SDL_WindowID windowID; /**< The window underneath the finger, if any */
.BI "} SDL_TouchFingerEvent;
